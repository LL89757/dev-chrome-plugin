{
  "manifest_version": 2, 
  "name": "开发环境登录插件", 
  "description": "用于开发环境登录", 
  "version": "0.1", 
  "browser_action": {
  },
  "background":
    {
    "page": "background/background.html" 
  },
  "content_scripts":[
		{
      "matches": ["<all_urls>"],
      "js":["content/content.js"],
			"run_at": "document_start"
		}
	],
  "permissions": ["storage","http://*/*", "https://*/*","notifications"]
}
